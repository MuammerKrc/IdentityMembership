@model List<System.Security.Claims.Claim>

@{
	ViewData["Title"] = "Index";
}





<h3>Users</h3>
<hr />

<a class="btn btn-primary" asp-action="AnkaraPolicyAuthorize">AnkaraCityPolicy</a>
<table class="table table-bordered table-striped">

	<tr>
		<td>Dağıtıcı</td>
		<td>Türü</td>
		<td>Değeri</td>
	</tr>
	@if (Model.Count() == 0)
	{
		<tr>
			<td colspan="3">Hiç bir üye yoktur.</td>
		</tr>
	}
	else
	{
		@foreach (var item in Model)
		{
			<tr>
				<td>@item.Issuer</td>
				<td>@item.Type</td>
				<td>@item.Value</td>
			</tr>
		}
	}
</table>
